{
  "name": "Analog.Cafe.next",
  "version": "0.0.1",
  "description": "Front-end SSR for Analog.Cafe built with Next.js",
  "main": "index.js",
  "author": "dmitrizzle",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "start": "NODE_ENV=production node server.js",
    "build": "next build",
    "size": "next build && size-limit",
    "lint": "eslint \"./**/*.js\"",
    "lint:fix": "eslint --fix \"./**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint:fix"
    }
  },
  "size-limit": [
    {
      "webpack": true,
      "path": ".next/static/chunks/*.js",
      "limit": "130 KB"
    }
  ],
  "dependencies": {
    "express": "^4.17.1",
    "next": "^8.1.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0",
    "styled-reset": "^2.0.13",
    "typeface-exo-2": "^0.0.72",
    "typeface-lora": "^0.0.72"
  },
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "husky": "^2.3.0",
    "size-limit": "^1.3.5",
    "url-loader": "^1.1.2"
  }
}

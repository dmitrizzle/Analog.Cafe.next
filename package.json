{
  "name": "Analog.Cafe.next",
  "version": "0.0.1",
  "description": "Front-end SSR for Analog.Cafe built with Next.js",
  "main": "index.js",
  "author": "dmitrizzle",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:travis": "npx cypress run  --spec \"cypress/integration/core/**/*\"",
    "cy:start-server-and-test": "npx start-server-and-test start :3000 cy:run",
    "cy:start-server-and-test:travis": "npx start-server-and-test start :3000 cy:run:travis",
    "cy:verify": "cypress verify",
    "dev": "node server.js",
    "lint": "eslint \"./**/*.js\"",
    "lint:fix": "eslint --fix \"./**/*.js\"",
    "start": "NODE_ENV=production node server.js",
    "size": "next build && size-limit",
    "test:cypress:travis": "yarn build && cross-env STRIP_SERVER_CONSOLE=true yarn cy:start-server-and-test:travis",
    "test:travis": "npm-run-all --parallel --silent test:unit test:cypress:travis",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint:fix && yarn test:unit"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^typeface": "<rootDir>/__mocks__/css.js"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js",
      "jest-localstorage-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "cypress"
    ]
  },
  "size-limit": [
    {
      "webpack": true,
      "path": ".next/static/chunks/*.js",
      "limit": "130 KB"
    }
  ],
  "dependencies": {
    "@next/bundle-analyzer": "^9.4.4",
    "@roast-cms/french-press-editor": "^14.0.0",
    "@roast-cms/image-froth": "^0.1.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "cacheable-response": "^2.0.7",
    "clipboard-polyfill": "^3.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "1.0.5",
    "isomorphic-unfetch": "^3.0.0",
    "keycode": "^2.2.0",
    "lodash": "^4.17.11",
    "lodash.throttle": "^4.1.1",
    "lscache": "^1.3.0",
    "next": "^9.4.2",
    "next-compose-plugins": "^2.2.0",
    "next-pwa": "^3.0.3",
    "next-seo": "^4.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^3.1.2",
    "react-lazyload": "^3.0.0",
    "react-redux": "^7.0.3",
    "react-textarea-autosize": "^8.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "size-limit": "^4.5.4",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.0.0",
    "styled-reset": "^4.0.2",
    "titlecase": "^1.1.3",
    "typeface-exo-2": "^1.1.4",
    "typeface-lora": "^0.0.72",
    "url-pattern-match": "^1.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.11.0",
    "cypress-plugin-retries": "^1.5.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^6.0.0",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-styled-components": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "redux-mock-store": "^1.5.3",
    "start-server-and-test": "^1.10.6",
    "url-loader": "^4.1.0"
  }
}
